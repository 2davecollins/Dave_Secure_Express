<div class="row">
  <div class="col-sm-8">
    <h1>Admin Page</h1>
   </div>
  <div class="col-sm-4">
    <a class="btn btn-primary" href="/storelocation" role="button">See All Stores</a>
    <a class="btn btn-primary" href="/admin/users" role="button">See All Users</a>
  </div>
</div>

<table class="table table-striped">
    <thead>
        <tr>
          <th scope="col">Level</th>
          <th scope="col">Message</th>
          <th scope="col">Time</th>        
        </tr>
      </thead>

      <tbody>
        <% for (var i = 0; i < loglist.length; i++) { %>
            <tr>
              <th scope="row"><%= loglist[i].level %></th>
              <th><%= loglist[i].message %></th>
              <th><%= new Date(loglist[i].timestamp).toUTCString() %></th>
            </tr>    
        <% } %>
      </tbody>    
 </table>

